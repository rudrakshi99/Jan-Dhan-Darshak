{
  "expo": {
    "name": "Jan-Dhan-Darshak",
    "slug": "Jan-Dhan-Darshak",
    "scheme" : "jandhandarshak",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist" : {
        "UIBackgroundModes" : [
          "location",
          "fetch",
          "audio"
        ]
      }
    },
    "android": {
      "package" : "com.jss.jandhandarshak",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["ACCESS_BACKGROUND_LOCATION", "ACCESS_FOREGROUND_LOCATION", "ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "RECORD_AUDIO", "LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
